{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . myApp --asar=true --out=test --overwrite --platform=win32 --arch=x64 --icon=clock.ico --prune=true --squirrel-install --ignore=node_modules/electron-* --electron-version=7.1.8 --ignore=AlarmClock-win32-x64 --version-string.CompanyName=TDI --version-string.ProductName=JANE",
    "packager": "electron-packager . HelloWorld --all --out ./OutApp --app-version 7.1.8 --overwrite --icon=clock.ico"
    },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.8",
    "electron-packager": "^14.2.0"
  },
  "dependencies": {}
}
